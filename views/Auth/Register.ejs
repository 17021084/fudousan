<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="/images/icon/040-home.svg">
    
    <title>Register</title>
    
    
    <link rel="stylesheet" href="/stylesheets/style.css">
    <link rel="stylesheet" href="/stylesheets/authen.css">
    <!-- bootstrap  -->
    <link rel="stylesheet" href="/javascripts/bootstrap/dist/css/bootstrap.css">
    <script src="https://cdn.ckeditor.com/ckeditor5/17.0.0/classic/ckeditor.js"></script>



</head>
<body>
  
  
  <!-- header -->
  <%- include ('../Header.ejs') %> 


   <div class="form">
        <form id="register" action="">
            <h4 style="text-align: center;">Register </h4>
            <div class="inputfield">
                  <div class="form-group">
                    <label for="Email">Email</label>
                    <input type="text"
                      class="form-control" name="Email" id="Email" aria-describedby="helpId" placeholder="abc@gmail.com" required>
                    <small id="Email" class="form-text text-muted">Help text</small>
                  </div>
                  <div class="form-group">
                    <label for="Password">Password</label>
                    <input type="password"
                      class="form-control" name="Password" id="Password" aria-describedby="helpId" required placeholder="Password">
                    <small id="Password" class="form-text text-muted">Help text</small>
                  </div>
                  <div class="form-group">
                    <label for="ConfirnPassword">Confirn Password</label>
                    <input type="password"
                      class="form-control" name="ConfirnPassword" id="ConfirnPassword" aria-describedby="helpId" required placeholder="Password">
                    <small id="ConfirnPassword" class="form-text text-muted">Help text</small>
                  </div>

                  <div class="form-group">
                    <label for="Phone">Phone</label>
                    <input type="text"
                      class="form-control" name="Phone" id="Phone" aria-describedby="Phone" placeholder="14024845" required>
                    <small id="Phone" class="form-text text-muted">Help text</small>
                  </div>
                  <div class="form-group">
                    <label for="Dob">Date Of Birth</label>
                    <input type="text"
                      class="form-control" name="Dob" id="Dob" aria-describedby="Dob" placeholder="dd/mm/yy" required>
                    <small id="Dob" class="form-text text-muted">Help text</small>
                  </div>
                    


                  <button  style="margin-left:40%;" type="button" class="btn btn-warning">Register </button>
                  
            </div>
            
                
        </form>  

   </div>
   
   <!-- footer -->
   <%- include ('../Footer.ejs') %> 


    <!-- include jquery and bootstap -->
    <script src="/javascripts/jquery/dist/jquery.js"></script>
    <script src="/javascripts/bootstrap/dist/js/bootstrap.js"></script>



    <script type="text/javascript" >
       
    

    </script>


</body>
</html>